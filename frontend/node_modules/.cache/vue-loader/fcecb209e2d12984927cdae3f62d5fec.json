{"remainingRequest":"/Users/susmitvengurlekar/Todo-App/frontend/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/susmitvengurlekar/Todo-App/frontend/src/components/TodoDetailsDialog.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/susmitvengurlekar/Todo-App/frontend/src/components/TodoDetailsDialog.vue","mtime":1604301866967},{"path":"/Users/susmitvengurlekar/Todo-App/frontend/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/susmitvengurlekar/Todo-App/frontend/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/susmitvengurlekar/Todo-App/frontend/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["TodoDetailsDialog.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqFA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"TodoDetailsDialog.vue","sourceRoot":"src/components","sourcesContent":["<template>\n    <v-dialog\n        max-width=\"300\"\n        v-model=\"getTodoDetailsDialogStatus\"\n        :persistent=\"true\"\n    >\n        <v-card>\n            <v-card-title class=\"headline\">\n                My Todo\n                <v-spacer></v-spacer>\n                <span class=\"caption\"> Priority : </span>\n                <v-icon :color=\"getPriorityColorMapping[todo.priority]\">mdi-label</v-icon>\n                \n                <v-dialog max-width=\"250\" :persistent=\"true\" v-model=\"dialog\">\n                    <template #activator=\"{on:dialog}\">\n                        <v-tooltip right>\n                                <template #activator=\"{on:tooltip}\">\n                                    <v-btn text v-on=\"{...tooltip,...dialog}\" fab>\n                                        <v-icon small>mdi-pencil</v-icon>\n                                    </v-btn>\n                                </template>\n                                <span>Edit Priority</span>\n                        </v-tooltip>\n                    </template>\n                    <v-card>\n                        <v-card-title>Choose New Priority</v-card-title>\n                        <v-card-text align=\"center\"\n                             v-for=\"priority in [3,2,1].filter((v)=>v!=todo.priority)\"\n                             :key=\"priority\"\n                        >\n                            <v-btn :color=\"getPriorityColorMapping[priority]\" @click=\"changePriorityTo(priority)\">\n                                {{priorityNameMapping[priority]}}\n                            </v-btn>\n                        </v-card-text>\n                        <v-divider></v-divider>\n                        <v-card-actions class=\"justify-center\">\n                            <v-btn @click=\"dialog=false\">\n                                Cancel\n                            </v-btn>\n                        </v-card-actions>\n                    </v-card>\n                </v-dialog>\n            </v-card-title>\n            <v-card-text class=\"mt-3\">\n                <v-form ref=\"form\" >\n                    <v-text-field\n                            label=\"Title\"\n                            filled\n                            :readonly=\"!getEditTodoStatus\"\n                            v-model=\"todo.title\"\n                            :rules=\"titleRules\"\n                            required\n                            />\n\n                        <v-textarea\n                            label=\"Description\"\n                            filled\n                            :readonly=\"!getEditTodoStatus\"\n                            v-model=\"todo.description\"\n                            :rules=\"descRules\"\n                            required\n                            />  \n                     </v-form>\n            </v-card-text>\n          \n            <v-card-actions>\n                <v-btn color=\"warning\" @click=\"editTodo\" v-if=\"!getEditTodoStatus\">\n                    Edit\n                </v-btn>\n                <v-spacer></v-spacer>\n                \n                <v-btn text color=\"purple\" @click=\"closeDialog\" v-if=\"!getEditTodoStatus\">\n                    Close\n                </v-btn>\n\n                <v-btn color=\"success\" @click=\"saveEditedTodo\" v-else>\n                    Save\n                </v-btn>\n            </v-card-actions>\n        </v-card>\n\n    </v-dialog>\n</template>\n\n<script>\nimport {mapGetters} from \"vuex\";\n\nexport default {\n    name : \"TodoDetailsDialog\",\n    computed : {\n        ...mapGetters([\"getTodoDetailsDialogStatus\",\"getEditTodoStatus\",\"getPriorityColorMapping\"]),\n        todo() { return this.$store.getters.getCurrentTodo},\n    },\n    methods : {\n        closeDialog(){\n            this.$store.commit(\"setTodoDetailsDialogStatus\",false)\n        },\n        editTodo(){\n            this.$store.commit(\"setEditTodoStatus\",true)\n        },\n        saveEditedTodo(){\n            if (this.$refs.form.validate()){\n              this.$store.commit(\"setEditTodoStatus\",false)\n            this.$store.dispatch(\"changeTodoDetails\")\n            }\n            \n        },\n        changePriorityTo(newPriority){\n            this.todo.priority = newPriority;\n            this.dialog=false;\n            this.$store.commit(\"setEditTodoStatus\",true)\n        }\n    },\n    data(){\n        return {\n            priorityNameMapping : {\n                \"3\" : \"High\",\n                \"2\" : \"Medium\",\n                \"1\" : \"Low\"\n            },\n            dialog : false,\n             titleRules : [\n                v => !!v || \"Title is required\",\n            ],\n            descRules : [\n                v => !!v || \"Describe here, free clutter in mind\",\n            ]\n        }\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"]}]}